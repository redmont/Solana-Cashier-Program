{
  "name": "zealy-module",
  "scripts": {
    "dev": "tsup --watch",
    "prebuild": "rm -rf dist",
    "zip": "find dist/ -type f -exec sh -c 'zip \"$(dirname \"$1\")/$(basename \"$1\" .js).zip\" \"$1\"' _ {} \\;",
    "build": "tsup && pnpm zip"
  },
  "devDependencies": {
    "@aws-sdk/client-servicediscovery": "^3.583.0",
    "@types/aws-lambda": "^8.10.138",
    "@types/node": "^20.12.12",
    "aws-lambda": "^1.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "nats": "^2.22.0"
  }
}
