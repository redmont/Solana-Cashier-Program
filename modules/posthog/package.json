{
  "name": "posthog-module",
  "scripts": {
    "dev": "tsup --watch",
    "prebuild": "rm -rf dist",
    "zip": "find dist/ -type f -exec sh -c 'zip \"$(dirname \"$1\")/$(basename \"$1\" .js).zip\" \"$1\"' _ {} \\;",
    "build": "tsup && pnpm zip"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.631.0",
    "@aws-sdk/client-s3": "^3.574.0",
    "@aws-sdk/util-dynamodb": "^3.631.0",
    "@types/aws-lambda": "^8.10.138",
    "@types/node": "^20.12.12",
    "aws-lambda": "^1.0.7",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "csv-stringify": "^6.5.1"
  }
}
